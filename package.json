{
    "name": "openseadragondeltaemap",
    "version": "1.0.1",
    "description": "Draws overlay over a targetscan, and shows calibration info according to deltaE standards",
    "main": "dist/openSeadragonDeltaEMap.js",
    "module": "module/openSeadragonDeltaEMap.js",
    "directories": {
        "test": "test"
    },
    "scripts": {
        "prebuild": "cf dist module && prettier '{./{src,test}/*.{js,scss},./demo/demo.js}' --write",
        "build": "rollup --config",
        "coverage": "cf coverage && jest --coverage",
        "once": "npm run test && npm run build",
        "pretest": "eslint {src,test/**}/*.js",
        "test": "jest",
        "watch": "onchange --initial \"./{src,test/**}/*.{js,scss}\" \"./demo/*.{js,json}\" -- npm run once --silent"
    },
    "keywords": [
        "openseadragon",
        "deltae",
        "delta-e",
        "map",
        "plugin",
        "overlay",
        "patch",
        "target",
        "front-end"
    ],
    "author": "Onno van der Zee",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/picturae/openseadragondeltaemap.git"
    },
    "bugs": {
        "url": "https://github.com/picturae/openseadragondeltaemap/issues"
    },
    "homepage": "https://github.com/picturae/openseadragondeltaemap#readme",
    "peerDependencies": {
        "openseadragon": "^2.1.0"
    },
    "dependencies": {
        "my-lib": "^1.2.0",
        "openseadragon": "^2.4.2",
        "transform-case": "^0.2.0"
    },
    "devDependencies": {
        "@babel/core": "^7.10.2",
        "@babel/preset-env": "^7.10.2",
        "@rollup/plugin-commonjs": "^11.1.0",
        "@rollup/plugin-node-resolve": "^7.1.3",
        "clear-folder": "^2.0.0",
        "coveralls": "^3.1.0",
        "eslint": "^6.5.1",
        "eslint-plugin-jasmine": "^2.10.1",
        "eslint-plugin-jest": "^22.20.0",
        "jest": "^25.5.4",
        "onchange": "^6.1.1",
        "prettier": "^1.18.2",
        "rollup": "^1.32.1",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-sass": "^1.2.2",
        "rollup-plugin-terser": "^5.3.0"
    }
}
